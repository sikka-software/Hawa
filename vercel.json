{
  "version": 2,
  "builds": [
    {
      "src": "packages/components/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/www/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/www/public/$1",
      "continue": true
    }
  ]
}
