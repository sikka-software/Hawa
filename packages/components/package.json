{
  "name": "@sikka/hawa",
  "version": "0.32.2",
  "description": "Modern UI Kit made with Tailwind",
  "author": {
    "name": "Sikka Software",
    "email": "contact@sikka.io",
    "url": "http://sikka.io"
  },
  "homepage": "https://hawa.style",
  "license": "MIT",
  "keywords": [
    "ui",
    "saas",
    "kit",
    "tailwind",
    "hawa"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sikka-software/hawa.git"
  },
  "bugs": {
    "url": "https://github.com/sikka-software/hawa/issues",
    "email": "hawa@sikka.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*/index.js"
    },
    "./blocks/*": {
      "import": "./dist/blocks/*/index.js"
    },
    "./dist/style.css": "./dist/index.css"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "test": "jest",
    "prebuild": "node ./util/pre-build.js",
    "build:lib": "npm run prebuild && node ./util/build-library.mjs",
    "build:css": "postcss styles.css -o dist/index.css",
    "build": "npm run build:lib && npm run build:css",
    "pub:next": "npm publish --tag next",
    "pub": "npm publish"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-table": "^8.17.3",
    "@tanstack/table-core": "^8.17.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "embla-carousel-auto-height": "^8.0.4",
    "embla-carousel-react": "^8.0.4",
    "eslint": "^9.2.0",
    "eslint-config-next": "^14.2.3",
    "libphonenumber-js": "^1.11.1",
    "prism-react-renderer": "^2.3.1",
    "prismjs": "^1.29.0",
    "react-dropzone": "^14.2.3",
    "react-headless-pagination": "^1.1.5",
    "react-hook-form": "^7.51.4",
    "react-select": "^5.8.0",
    "signature_pad": "^5.0.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "trim-canvas": "^0.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.1",
    "@sikka/alam": "^0.0.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "embla-carousel": "8.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.378.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsup": "^8.0.2"
  }
}
