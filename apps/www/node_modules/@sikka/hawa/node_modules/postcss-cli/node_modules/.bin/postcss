#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/postcss-cli@11.0.0_postcss@8.4.33/node_modules/postcss-cli/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/postcss-cli@11.0.0_postcss@8.4.33/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/postcss-cli@11.0.0_postcss@8.4.33/node_modules/postcss-cli/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/postcss-cli@11.0.0_postcss@8.4.33/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
