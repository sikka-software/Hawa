#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules/tsup/dist/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules/tsup/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules/tsup/dist/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules/tsup/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/tsup@8.0.1_@swc+core@1.3.102_postcss@8.4.33_typescript@5.3.3/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
