#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer/bin/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer/bin/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules/contentlayer/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.17.19_markdown-wasm@1.2.0/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.cjs" "$@"
else
  exec node  "$basedir/../../bin/cli.cjs" "$@"
fi
