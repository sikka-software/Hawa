#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/cjs/src/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/cjs/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/cjs/src/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/cjs/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/dist/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules/rimraf/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/rimraf@4.4.1/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/src/bin.js" "$@"
fi
