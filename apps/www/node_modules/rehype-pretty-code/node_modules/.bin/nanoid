#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules/nanoid/bin/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules/nanoid/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules/nanoid/bin/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules/nanoid/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/nanoid@4.0.2/node_modules:/mnt/c/Users/Zakher/Desktop/coding/hawa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@4.0.2/node_modules/nanoid/bin/nanoid.js" "$@"
else
  exec node  "$basedir/../../../../../nanoid@4.0.2/node_modules/nanoid/bin/nanoid.js" "$@"
fi
